{%- assign current_variant = product.selected_or_first_available_variant -%}



<div class="woocommerce" id="section-{{ section.id }}">
  <div class="axil-single-product-area axil-section-gap pb--0 bg-color-white single-product-1">
    <div class="single-product-thumb">
      <div class="{% if section.settings.container_style == 'style_1' %}container p-0 {% else %} container-fluid p-0{% endif %}">
        <div class="row {% if section.settings.content_handle == 'content_in_left' %}flex-row-reverse{% endif %}">
          {%- if section.settings.product_style == 'product_default' -%}
            <div class="col-lg-6 mb--40">
              <div class="h-100">
                <div class="position-sticky sticky-top">
                    <div class="row row--10">
                      {%- if product.images.size -%}
                        {% for image in product.images %}
                        <div class="col-6 mb--20">
                            <div class="single-product-thumbnail axil-product thumbnail-grid">
                              <div class="thumbnail"> 
                                    <figure class="product-gallery__thumb--single">
                                      <a class="product-single__thumbnail {% if forloop.first and section.settings.product_zoom_enable %}active{% endif %}" href="{{ image.src | img_url: '1024x1024' }}" 
                                      data-image="{{ image.src | img_url: '1024x1024' }}" data-zoom-image="{{ image.src | img_url: '1024x1024' }}" data-image-id="{{ image.id }}">
                                      <img src="{{ image.src | img_url: 'compact' }}" alt="{{ image.alt | escape }}">
                                      </a>
                                  </figure>
                              </div>
                            </div>
                        </div>
                        {% endfor %}
                      {%- endif -%}
                        <!-- End .col -->
                    </div>
                </div>
            </div>
            </div> 
          {%- endif -%}

          <div class="col-lg-6 mb--40">
            <div class="h-100">
              <div class="position-sticky sticky-top">
                  <div class="single-product-content">
                      <div class="inner">
                        {%- assign product_form_id = 'product-form-' | append: section.id -%}
                        {%- for block in section.blocks -%}
                          {%- case block.type -%}
                            {%- when '@app' -%}
                              {% render block %}

                          {%- when 'text' -%}
                            {% if block.settings.text != blank %}
                            <div class="product-info-main-block product-item-vendor text-{{ block.settings.text-transform }}">
                                {{ block.settings.text }}
                              </div>
                            {% endif %}
                        
                          {%- when 'title' -%}
                            <div class="product-info-main-block product-title-wrapper">
                              <h1 class="product-title text-{{ block.settings.text_transform }}">{{ product.title }}</h1>
                            </div>

                            {%- when 'price' -%}
                              <div class="product-info-main-block price-box no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                                <div class="product-price-wrapper mb--20 mb-md--10">
                                  <span class="money price-amount" id="ProductPrice">{{ product.selected_or_first_available_variant.price | money }}</span>
                                   {%- if product.compare_at_price_max > product.price -%}
                                  <span class="old-price">
                                    <span class="money price-amount" id="ComparePrice">{{ product.selected_or_first_available_variant.compare_at_price | money }}</span>
                                  </span>
                                   {%- endif -%}
                                </div>
                              </div> 
                           
                              {%- when 'product_short_description' -%} 
                                <div class="product-short-description description mb--45 mb-sm--20">
                                  {{ product.metafields.custom.short_desciption | capitalize }}
                                </div>
            
                              
                            {%- when 'buy_buttons' -%}
                              <div class="product-info-main-block box-tocart">
                              <product-form class="product-form">

                            <div class="product-form__error-message-wrapper" role="alert" hidden>
                              {% render 'icon-error' %}
                              <span class="product-form__error-message"></span>
                            </div>

                          {%- form 'product', product, id: product_form_id, class: 'form box-tocart', novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
                            <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}" disabled>
                            
                            <div class="product-action-wrapper d-flex product-form__buttons qty">
                              {%- for block in section.blocks -%}
                                {%- case block.type -%}
                                  {%- when 'buy_buttons' -%}

                                    {% if block.settings.show-quantity-selector %}

                                    <div class="control product-action-wrapper d-flex-center quantity">
                                      <div class="product-form__input product-form__quantity{% if settings.inputs_shadow_vertical_offset != 0 and settings.inputs_shadow_vertical_offset < 0 %} product-form__quantity-top{% endif %}" {{ block.shopify_attributes }}>
                                        <label class="form__label visually-hidden" for="quantity-{{ section.id }}">{{ 'products.product.quantity.label' | t }}</label>
                                        
                                        <quantity-input class="quantity d-flex input-group-btn pro-qty mr--20">
                                         
                                          <button class="quantity__button no-js-hidden icon-minus qty-minus" name="minus" type="button">
                                            <span class=" qtybtn quantity-btn quantity-minus">-</span>
                                          </button>

                                              <input class="quantity__input form-control input-text qty text" type="number" name="quantity" id="quantity-{{ section.id }}" min="1" value="1" form="{{ product_form_id }}">
                                          
                                            <button class="quantity__button no-js-hidden icon-plus qty-plus input-text qty text" name="plus" type="button">
                                              <span class=" qtybtn quantity-btn quantity-minus">+</span>
                                            </button>
                                        </quantity-input>

                                      </div>
                                    </div>
                                    {% endif %}



                                    {% if block.settings.add-to-cart-button %}
                                    <div class="actions">
                                        <button type="submit" name="add" class="product-form-submit button btn alt btn-secondary d-block w-100"
                                        {% if settings.cart_type == "drawer" %}
                                          data-bs-toggle="offcanvas"
                                          data-bs-target="#cart-drawer-popup"
                                          {% endif %}
                                          aria-controls="cart-drawer-popup"
                                          {%- if product.selected_or_first_available_variant.available == false or product.selected_or_first_available_variant.quantity_rule_soldout -%}
                                          disabled
                                          {%- endif -%}
                                          >
                                          <span>
                                          {%- if product.selected_or_first_available_variant.available == false or product.selected_or_first_available_variant.quantity_rule_soldout -%}
                                          {{ 'products.product.sold_out' | t }}
                                          {% else %}
                                          {% if product.selected_or_first_available_variant.inventory_quantity <= 0 %}
                                          {{ 'products.product.pre_order' | t }}
                                          {% else %}
                                          {{ 'products.product.add_to_cart' | t }}
                                          {% endif %}
                                          {% endif %}
                                        </span>
                                        <div class="loading-overlay__spinner hidden">
                                          <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                                            <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                                          </svg>
                                        </div>

                                      </button>
                                    </div>
                                    {% endif %}
 

                                  </div>
 
                                    <div class="addon-button-wrp">
                                      <ul class="addon-button-list">
                                        {%- when 'addons' -%}
                                        {% if block.settings.show_wishlist %}
                                            <li class="product-wishlist">
                                                <a href="javascript:void(0);" class="wishlist-button" data-product-handle="{{ product.handle }}" button-wishlist>
                                                    <span class="addon-icon-wrp">{% render 'icon', name: 'heart', class: 'w-24 h-24', size: 'custom' %}</span>
                                                    <span>Wishlist</span>
                                                </a>
                                            </li>
                                        {% endif %}
                                    {% if block.settings.show_compare %}
                                      <li class="product-compare">
                                          <a href="javascript:void(0);" class="add-to-compare" data-handle-product="{{ product.handle }}" button-compare>
                                            <span class="addon-icon-wrp">{% render 'icon', name: 'compare', class: 'w-24 h-24', size: 'custom' %}</span>
                                              <span>Compare</span>
                                          </a>
                                      </li>
                                  {% endif %}
                                  {%- if block.settings.show_atp -%}
                                      <li class="theme-ask">
                                          <a data-bs-toggle="modal" role="button" href="#contact-form-model">
                                              <span class="addon-icon-wrp"> {% render 'icon', name: 'question-icon', class: 'w-24 h-24', size: 'custom' %}</span>
                                              <span>Ask about this product</span>
                                          </a>
                                      </li>
                                  {%- endif -%}
                                   
                                      </ul>
                                    </div>
                                {%- endcase -%}
                              {%- endfor -%}
                          {%- endform -%}
                        </product-form>
                                          
                      </div>
                         
                      <div class="product-action-wrapper"> 
                      {% when 'meta' %}
                        {% render 'product-meta', block: block %}
                      </div> 

                        {% when 'social_share' %}  
                          {%- render 'social-share', block: block -%} 


                    {%- endcase -%}
                  {%- endfor -%}

                  </div>
              </div>
          </div>
          </div>
  
        </div>
      </div>
    </div>
  
  </div> <!-- end axil-single-product-area -->

</div> 
</div> 



