

<div class="axil-breadcrumb-area">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-8">
                <div class="inner"> 
                    <ul id="breadcrumbs" class="axil-breadcrumb liststyle d-flex">
                    </ul>
                    <h3 class="title">Blogs Post</h3>
                         
                </div>
            </div>
            <div class="col-lg-6 col-md-4">
                <div class="inner">
                        <div class="bradcrumb-thumb">
                           <img width="126" height="120" src="http://etrade.local/wp-content/uploads/2023/01/product-45.png" class="attachment-full size-full" alt=""> 
                        </div>
                    </div>
             </div>
        </div>
    </div>
</div>

 

{% schema %}
{
    "name": "Blog",
    "settings": [
        {
            "type": "text",
            "id": "blog_read_more",
            "label": "Read more label",
            "default": "Read more"
        },
        {
            "type": "range",
            "id": "number_of_blog_posts",
            "step": 1,
            "min": 1,
            "max": 6,
            "label": "Blog posts per page",
            "default": 3
        }
    ]
}
{% endschema %}